<nav class="level">
    <div class="level-left">

        <div class="level-item">
            <h1>Current Users</h1>
        </div>
    </div>
    <div class="level-right">
        <div class="level-item">
            <a class="button is-success" href="/new">Get Started</a>
        </div>
    </div>
</nav>
<div class="tile is-ancestor">

    {{#each users}}
    <div class="tile is-child box">
        <p class="title">{{name}}</p>
        <p>Likes: {{schedule.days.0.events.0.performer}} and {{schedule.days.1.events.1.performer}}!
        </p>
        <a class="button" href="/views/user/show.hbs">Schedule</a>
    </div>
    {{/each}}
    <div class="tile is-child box">

    </div>

</div>